{
  "name": "edge-native-ai",
  "version": "2.1.0",
  "description": "Decentralized Edge-Native AI Platform with Full Ruvnet Ecosystem Integration",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:api": "cd src/api && uvicorn main:app --host 0.0.0.0 --port 8000 --reload",
    "start:gateway": "node src/gateway/index.js",
    "start:ruvnet": "node src/ruvnet-ecosystem/index.js",

    "update-ruvnet": "ncu -u -f '/^@?ruv|^agentic-flow|^claude-flow|^agentdb|^ruvector|^strange-loops|neural-trader/'",
    "check-updates": "ncu -f '/^@?ruv|^agentic-flow|^claude-flow|^agentdb|^ruvector|^strange-loops|neural-trader/'",

    "setup": "npm install && npm run check-docker && npm run check-wasmedge",
    "setup:distributed": "node scripts/setup-distributed.js",
    "setup:kairos": "./infrastructure/kairos/generate-token.sh",

    "check-docker": "docker --version || echo 'Docker not installed'",
    "check-wasmedge": "wasmedge --version || echo 'WasmEdge not installed'",
    "check-k3s": "k3s --version || echo 'K3s not installed'",

    "start:agent": "node src/orchestration/start-agent.js",
    "start:swarm": "node src/ruvnet-ecosystem/swarm-demo.js",
    "start:quad": "node src/orchestration/quad-orchestrator.js",
    "start:qdag": "node src/orchestration/qdag-orchestrator.js",
    "start:gaianet": "gaianet start",
    "stop:gaianet": "gaianet stop",

    "sandbox:test": "node src/sandbox/test-sandbox.js",
    "sandbox:e2b": "node src/e2b/sandbox.js",

    "cluster:init": "node src/distributed/init-cluster.js",
    "cluster:status": "node src/distributed/cluster-status.js",

    "k8s:apply": "kubectl apply -f infrastructure/k3s/",
    "k8s:delete": "kubectl delete -f infrastructure/k3s/",
    "spinkube:deploy": "kubectl apply -f infrastructure/spinkube/",

    "test": "node --test src/**/*.test.js",
    "test:ruvnet": "node --test src/ruvnet-ecosystem/**/*.test.js",
    "lint": "eslint src/",
    "build:wasm": "cargo build --target wasm32-wasi --release"
  },
  "keywords": [
    "ai",
    "agents",
    "ruvnet",
    "agentic-flow",
    "claude-flow",
    "agentdb",
    "ruvector",
    "ruv-swarm",
    "edge-native",
    "kairos",
    "spinkube",
    "k3s",
    "kubernetes",
    "wasm",
    "webassembly",
    "wasmedge",
    "gaianet",
    "local-ai",
    "sovereign-ai",
    "a2a-protocol",
    "mcp",
    "litellm",
    "mlx",
    "apple-silicon",
    "distributed",
    "decentralized",
    "neural-networks",
    "strange-loops",
    "multi-agent"
  ],
  "author": "Edge-Native AI Team",
  "license": "MIT",
  "dependencies": {
    "agentic-flow": "^1.10.2",
    "claude-flow": "^2.7.41",
    "agentdb": "^1.6.1",
    "ruvector": "latest",

    "ruv-swarm": "latest",
    "@ruv/quad": "latest",
    "@ruv/qdag": "latest",
    "ruvllm": "latest",

    "strange-loops": "latest",
    "strange-loops-mcp": "latest",
    "@ruvnet/strange-loop": "latest",

    "sublinear-time-solver": "latest",
    "temporal-neural-solver": "latest",

    "@ruvnet/bmssp": "latest",

    "neural-trader": "latest",

    "@ruv/sparc-ui": "latest",
    "@agentics.org/sparc2": "^2.0.25",
    "create-sparc": "latest",

    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "ws": "^8.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",

    "better-sqlite3": "^11.3.0",
    "ioredis": "^5.4.1",
    "pg": "^8.12.0",

    "node-machine-id": "^1.1.12",
    "uuid": "^10.0.0",
    "crypto-js": "^4.2.0",

    "@modelcontextprotocol/sdk": "^0.5.0",
    "@alfonsograziano/node-code-sandbox-mcp": "^1.0.0"
  },
  "devDependencies": {
    "npm-check-updates": "^17.1.3",
    "nodemon": "^3.1.7",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edge-native-ai/agentic-local"
  }
}
